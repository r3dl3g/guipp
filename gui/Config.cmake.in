@PACKAGE_INIT@

set_and_check(GUIPP_LIBRARIES @GUIPP_LIBRARIES@)
set_and_check(GUIPP_APP_LIBRARIES @GUIPP_APP_LIBRARIES@)
set_and_check(GUIPP_SYS_LIBRARIES @GUIPP_SYS_LIBRARIES@)
set(GUIPP_CXX_FLAGS @GUIPP_CXX_FLAGS@)
set(GUIPP_EXE_LINKER_FLAGS @GUIPP_EXE_LINKER_FLAGS@)
set(GUIPP_CXX_STANDARD @GUIPP_CXX_STANDARD@)

include("${CMAKE_CURRENT_LIST_DIR}/gui++Targets.cmake")

set(_supported_components core io draw win layout app ctrl look)

foreach(_comp ${gui++_FIND_COMPONENTS})
  if (NOT _comp IN_LIST _supported_components)
    set(gui++_FOUND False)
    set(gui++_NOT_FOUND_MESSAGE "Unsupported component: ${_comp}")
  endif()
  include("${CMAKE_CURRENT_LIST_DIR}/gui++${_comp}Targets.cmake")
endforeach()
